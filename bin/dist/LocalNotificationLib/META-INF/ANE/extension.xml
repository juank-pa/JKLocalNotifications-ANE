<extension xmlns="http://ns.adobe.com/air/extension/26.0">
    <id>com.juankpro.ane.LocalNotification</id>
    <versionNumber>1.0.0</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>localnotification.jar</nativeLibrary>
                <initializer>com.juankpro.ane.localnotif.Extension</initializer>
                <finalizer>com.juankpro.ane.localnotif.Extension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libLocalNotificationLib.a</nativeLibrary>
                <initializer>ComJkLocalNotificationExtInitializer</initializer>
                <finalizer>ComJkLocalNotificationExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
